<!-- app/views/age_tables/new.html.erb -->
<div class="container mt-5">
  <%= form_with(model: @age_table, url: age_tables_path) do |form| %>
    <div class="form-group">
      <%= form.label :table_title, '表の名前' %>
      <%= form.text_field :table_title, class: 'form-control' %>
    </div>
    <%= form.submit '作成する', class: 'btn btn-primary' %>
  <% end %>
</div>
